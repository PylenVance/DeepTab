{
  "manifest_version": 3,
  "name": "deeptab",
  "version": "1.0",
  "description": "A minimalist Safari-inspired new tab page",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["storage"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}